			"fontFamilies": [
				{
					"fontFamily": "-apple-system, BlinkMacSystemFont, 'Segoe UI', Roboto, Oxygen-Sans, Ubuntu, Cantarell, 'Helvetica Neue', sans-serif",
					"name": "System Font",
					"slug": "system-font"
				},
				{
					"fontFamily": "Krona One",
					"slug": "krona-one",
					"fontFace": [
						{
							"fontFamily": "Krona One",
							"fontStyle": "normal",
							"fontWeight": "400",
							"src": [
								"file:./assets/fonts/krona-one_normal_400.ttf"
							]
						}
					]
				},
				{
					"fontFamily": "Noto Sans",
