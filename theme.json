			],
			"fontSizes": [
				{
					"fluid": false,
					"name": "Base",
					"size": "1.125rem",
					"slug": "base"
				}
			]
		},
		"color": {
			"defaultPalette": true,
			"defaultGradients": false,
			"defaultDuotone": false,
			"palette": [
				{
					"name": "Base",
					"slug": "base",
					"color": "#1F2730"
				},
				{
					"name": "Contrast",
					"slug": "contrast",
					"color": "#EFEFEF"
				}
			]
		},
		"custom": {
			"post-title": {
				"typography": {
					"letterSpacing": "-0.24rem"
				}
			},
			"core/navigation": {
				"modal": {
					"color": {
						"background": "var(--wp--preset--color--base)"
					},
					"opacity": 0.8,
					"filter": "blur(12.5px)"
				}
			}
		}
	},
	"styles": {
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--noto-sans)",
			"fontWeight": "400",
			"fontSize": "var(--wp--preset--font-size--base)"
		},
		"color": {
			"text": "var(--wp--preset--color--contrast)",
			"background": "var(--wp--preset--color--base)"
		},
		"elements": {
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--krona-one)",
					"fontWeight": "400"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--contrast)"
				}
			}
		},
		"spacing": {
			"blockGap": "var(--wp--preset--spacing--30)",
			"padding": {
				"left": "var(--wp--preset--spacing--25)",
				"right": "var(--wp--preset--spacing--25)",
				"top": "var(--wp--preset--spacing--180)",
				"bottom": "var(--wp--preset--spacing--180)"
			}
		},
		"blocks": {
			"core/post-title": {
				"typography": {
					"letterSpacing": "var(--wp--custom--post-title--typography--letter-spacing)"
				}
			},
			"core/query-title": {
				"typography": {
					"letterSpacing": "var(--wp--custom--post-title--typography--letter-spacing)"
				}
			}
		}
	},
	"templateParts": [
		{
			"area": "header",
			"name": "header",
			"title": "Header"
		},
		{
			"area": "footer",
			"name": "footer",
			"title": "Footer"
		}
	],
	"version": 2
}