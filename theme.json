	"styles": {
		"typography": {
			"fontFamily": "var(--wp--preset--font-family--noto-sans)",
			"fontWeight": "400",
			"fontSize": "var(--wp--preset--font-size--base)",
			"letterSpacing": "0.0225rem",
			"lineHeight": "1.875rem"
		},
		"color": {
			"text": "var(--wp--preset--color--contrast)",
			"background": "var(--wp--preset--color--base)"
		},
		"elements": {
			"heading": {
				"typography": {
					"fontFamily": "var(--wp--preset--font-family--krona-one)",
					"fontWeight": "400",
					"letterSpacing": "0",
					"lineHeight": "normal"
				},
				"spacing": {
					"margin": {
						"top": "var(--wp--preset--spacing--60)"
					}
				}
			},
			"h1": {
				"typography": {
					"letterSpacing": "-0.18rem",
					"fontSize": "var(--wp--preset--font-size--80)"
				}
			},
			"h2": {
				"typography": {
					"letterSpacing": "-0.12375rem",
					"fontSize": "var(--wp--preset--font-size--60)"
				}
			},
			"h3": {
				"typography": {
					"letterSpacing": "-0.1125rem",
					"fontSize": "var(--wp--preset--font-size--base)"
				}
			},
			"h4": {
				"typography": {
					"letterSpacing": "-0.1rem",
					"fontSize": "var(--wp--preset--font-size--40)"
				}
			},
			"h5": {
				"typography": {
					"letterSpacing": "-0.0875rem",
					"fontSize": "var(--wp--preset--font-size--30)"
				}
			},
			"h6": {
				"typography": {
					"letterSpacing": "-0.075rem",
					"fontSize": "var(--wp--preset--font-size--20)"
				}
			},
			"link": {
				"color": {
					"text": "var(--wp--preset--color--accent)"
				},
				"typography": {
					"textDecoration": "underline"
				},
				":hover": {
					"color": {
						"text": "var(--wp--preset--color--contrast)"
					},
					"typography": {
						"textDecoration": "none"
					}
				},
				":focus": {
					"color": {
						"text": "var(--wp--preset--color--contrast)"
					},
					"typography": {
						"textDecoration": "none"
					}
				},
				":active": {
					"color": {
						"text": "var(--wp--preset--color--black)"
					},
					"typography": {
						"textDecoration": "none"
					}
				}
			}
		},
		"spacing": {
			"blockGap": "var(--wp--preset--spacing--30)",
			"padding": {
				"left": "var(--wp--preset--spacing--25)",
				"right": "var(--wp--preset--spacing--25)"
			},
			"margin": {
				"top": "var(--wp--preset--spacing--180)",
				"bottom": "var(--wp--preset--spacing--180)"
			}
		},
