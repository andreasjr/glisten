{
	"$schema": "https://schemas.wp.org/wp/6.4/theme.json",
	"settings": {
		"appearanceTools": true,
		"useRootPaddingAwareAlignments": true,
		"layout": {
			"contentSize": "750px",
			"wideSize": "1000px"
		},
		"spacing": {
			"padding": true,
			"margin": true,
			"customSpacingSize": true,
			"units": [
				"%",
				"px",
				"em",
				"rem",
				"vh",
				"vw"
			],
			"spacingScale": {
				"steps": 0
			},
			"spacingSizes": [
				{
					"name": "25",
					"size": "1.5625rem",
					"slug": "25"
				},
				{
					"name": "30",
					"size": "1.875rem",
					"slug": "30"
				},
				{
					"name": "90",
					"size": "5.625rem",
					"slug": "90"
				},
				{
					"name": "180",
					"size": "11.25rem",
					"slug": "180"
				}
			]
		},
